<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cpm_odoo.document_create_form_view" model="ir.ui.view">
            <field name="name">cpm_odoo.document_create_form_view</field>
            <field name="model">cpm_odoo.documents_document</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>
                        <button class="btn btn-secondary" special="cancel" onclick="window.history.back()">
                            Back
                        </button>
                    </header>
                    <sheet>
                        <group col="3">
                            <group colspan="2">
                                <field name="document_set_id" string="Document Set"></field>
                                <field name="file" string="File"></field>
                            </group>
                            <group colspan="1">
                                
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>    
    </data>
</odoo>
